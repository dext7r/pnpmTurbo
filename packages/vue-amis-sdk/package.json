{
  "name": "vue-amis-sdk",
  "version": "2.7.0",
  "author": "h7ml <h7ml@qq.com>",
  "private": true,
  "license": "MIT",
  "main": "lib/vue-amis-sdk.umd.min.js",
  "description": "vue amis sdk",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "rimraf lib && vue-cli-service build --target lib --formats umd-min -name vue-amis-sdk --dest lib packages/index.js --report && cp ./examples/index.html ./lib/"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "amis": "2.7.3-beta.0",
    "amis-editor": "5.2.4-beta.31",
    "amis-editor-core": "5.2.4-beta.31",
    "amis-ui": "2.7.3-beta.0",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^2.6.14",
    "vuera": "^0.2.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "chalk": "^5.2.0",
    "cz-git": "1.5.0",
    "file-loader": "^6.2.0",
    "postcss": "8.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^4.1.2",
    "sass": "~1.54.2",
    "sass-loader": "^10.2.0",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./.commitlintrc.js"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
