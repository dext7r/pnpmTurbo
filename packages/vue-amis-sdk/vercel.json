{
  "rewrites": [
    {
      "source": "/amis/(.*)",
      "destination": "/api/amis"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/antd"
    }
  ],
  "headers": [
    {
      "source": "/(.*).(ttf|otf|woff2|woff|eot|json)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
